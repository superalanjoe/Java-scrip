// let é uma variavel
let nome 
// atribuir valor ou inicializar variavel 
nome = "Alan"
console.log(nome)
sobrenome = "Cleber"
console.log(sobrenome)
// para tramsformar tudo em comentario é crtl+:^
// para declarar uma varial usa-se let
let cpf
//atribuindo um valor 
